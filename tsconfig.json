{
  "compilerOptions": {
    "target": "ES2020",
    "jsx": "react-jsx",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "removeComments": false,
    "noEmit": false,
    "noImplicitAny": true,
  }
}